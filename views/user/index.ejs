<div class="container">
  <h3>Users</h3>
  <table class='table'>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
      <th>Mobile Number</th>
    </tr>
    <%console.log(users);%>
    <%_.each(users , function(user){%>
    <tr data-id="<%= user.id%>" data-model="user">
      <td><%= user.fName%></td>
      <td><%= user.lName%></td>
      <td><%= user.email %></td>
      <td><%= user.mNumber %></td>
    </tr>
    <%})%>
  </table>
</div>
